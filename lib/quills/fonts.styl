font(nombre source)
  @font-face {
    font-family: nombre
    src:         url(source + '.eot')
    src:         url(source + '.eot#iefix') format('embedded-opentype'),
                 url(source + '.woff')      format('woff'),
                 url(source + '.ttf')       format('truetype'),
                 url(source + '.svg')       format('svg');
    font-weight: normal;
    font-style:  normal;
  }